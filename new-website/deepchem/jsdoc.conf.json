{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "components"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "better-docs/component"
  ],
  "templates": {
    "better-docs": {
      "name": "My React components"
    },
    "default": {
      "includeDate": false
    }
  },
  "opts": {
    "destination": "../docs",
    "recurse": true,
    "readme": "README.md"
  }
}